{
  "name": "docassemble_schema",
  "version": "0.1.0",
  "description": "A JSON schema for the docasseble platform",
  "main": "index.js",
  "scripts": {
    "cli": "npm run valid; npm run invalid",
    "valid": "ajv test -s=docassemble_schema.json -d=tests/valid/**/*.yml --valid --strict=false --spec=draft2020",
    "invalid": "ajv test -s=docassemble_schema.json -d=tests/invalid/**/*.yml --invalid --strict=false --spec=draft2020",
    "ajv": "node ./test_ajv.js --verbose=false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plocket/docassemble_schema.git"
  },
  "keywords": [
    "json",
    "schema",
    "docassemble"
  ],
  "author": "plocket",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plocket/docassemble_schema/issues"
  },
  "homepage": "https://github.com/plocket/docassemble_schema#readme",
  "devDependencies": {
    "@segment/ajv-human-errors": "^2.11.3",
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0",
    "better-ajv-errors": "^1.2.0",
    "chai": "^4.3.10",
    "jsonschema": "^1.4.1",
    "minimist": "^1.2.8"
  }
}
